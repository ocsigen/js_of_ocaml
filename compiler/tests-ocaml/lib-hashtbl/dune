(tests
 (names hfun)
 (deps ../../../LICENSE)
 (modules hfun)
 (libraries)
 (modes js))

(tests
 (names htbl)
 (deps ../../../LICENSE)
 (modules htbl)
 (build_if
  (>= %{ocaml_version} 5))
 (libraries)
 (modes js))

(rule
 (action
  (copy hfun.expected-js hfun.expected))
 (enabled_if
  (and
   (<> %{profile} wasm)
   (<> %{profile} wasm-effects))))

(rule
 (action
  (copy hfun.expected-wasm hfun.expected))
 (enabled_if
  (or
   (= %{profile} wasm)
   (= %{profile} wasm-effects))))
